<h1>Registro</h1>
<form action="" id="formRegister">
  <label for="input_first_name">Nombre</label> <input id="input_first_name" type="text"
    placeholder="first_name"><br>
  <label for="input_last_name">Apellido</label> <input id="input_last_name" type="text" placeholder="last_name"><br>
  <label for="input_email">Email</label> <input id="input_email" type="email" placeholder="email"><br>
  <label for="input_age">Edad</label> <input id="input_age" type="number" placeholder="age"><br>
  <label for="input_password">Password</label> <input id="input_password" type="password" placeholder="password"><br>
  <button type="submit">Enviar</button>
</form>

<script src="/js/register.js"></script>